@model ContactDto
<style>


    .contactContainer{
        display:flex;
        align-items:center;
        flex-wrap:wrap;
        justify-content:center;
        
    }

    .contactItem{
        margin-top:20px !important;
        display:flex;
        flex-direction:column;
        align-items:center;
    }

    @@import url('https://fonts.googleapis.com/css?family=Nunito:200,300,400,500,600,700,800,900,200i,300i,400i,500i,600i,700i,800i,900i&display=swap');
    @@import url('https://fonts.googleapis.com/css?family=Rajdhani:300,400,500,600,700&display=swap');

    @@media all {
        body {
            font-family: var(--bs-body-font-family);
            font-size: var(--bs-body-font-size);
            font-weight: var(--bs-body-font-weight);
            line-height: var(--bs-body-line-height);
            color: var(--bs-body-color);
            text-align: var(--bs-body-text-align);
        }
    }

    body {
        /* CSS Variables that may have been missed get put on body */
        --bs-gutter-x: 1.5rem;
        --bs-gutter-y: 0;
    }

    @@media all {
        * {
            box-sizing: border-box;
        }

        body {
            margin: 0;
            font-family: var(--bs-body-font-family);
            font-size: var(--bs-body-font-size);
            font-weight: var(--bs-body-font-weight);
            line-height: var(--bs-body-line-height);
            color: var(--bs-body-color);
            text-align: var(--bs-body-text-align);
            background-color: var(--bs-body-bg);
            -webkit-text-size-adjust: 100%;
            -webkit-tap-highlight-color: transparent;
        }

        body {
            overflow-x: hidden;
        }

        body {
            font-size: 16px;
            line-height: 26px;
            color: #fff;
            font-weight: 400;
            text-align: left;
            font-family: "Nunito", sans-serif;
        }

        html {
            overflow-x: hidden;
        }

        :root {
            --bs-font-sans-serif: system-ui,-apple-system,"Segoe UI",Roboto,"Helvetica Neue","Noto Sans","Liberation Sans",Arial,sans-serif,"Apple Color Emoji","Segoe UI Emoji","Segoe UI Symbol","Noto Color Emoji";
            --bs-body-font-family: var(--bs-font-sans-serif);
            --bs-body-font-size: 1rem;
            --bs-body-font-weight: 400;
            --bs-body-line-height: 1.5;
            --bs-body-color: #fff;
            --bs-body-bg: #fff;
            --bs-heading-color: inherit;
            --bs-border-radius-xxl: 2rem;
            --bs-border-radius-2xl: var(--bs-border-radius-xxl);
        }
    }

    @@media (prefers-reduced-motion: no-preference) {
        :root {
            scroll-behavior: smooth;
        }
    }

    @@media all {
        .row-2fv {
            --bs-gutter-x: 1.5rem;
            --bs-gutter-y: 0;
            display: flex;
            flex-wrap: wrap;
            margin-top: calc(-1 * var(--bs-gutter-y));
            margin-right: calc(-.5 * var(--bs-gutter-x));
            margin-left: calc(-.5 * var(--bs-gutter-x));
        }

        .item-2d4 {
            align-items: center !important;
        }

        .add-white-9pe {
            padding: 39px 37px 38px;
            border-radius: 10px;
            background-color: #ffffff;
            margin: -313px 0;
            position: relative;
        }

        .add-white-9pe {
            margin: 0 0;
        }

        *, :after, :before {
            box-sizing: border-box;
        }

        :selection {
            background: #4fc1f0;
            color: #454545;
            text-shadow: none;
        }

        .row-2fv > * {
            flex-shrink: 0;
            width: 100%;
            max-width: 100%;
            padding-right: calc(var(--bs-gutter-x) * .5);
            padding-left: calc(var(--bs-gutter-x) * .5);
            margin-top: var(--bs-gutter-y);
        }
    }

    @@media (min-width: 768px) {
        .col-42m {
            flex: 0 0 auto;
            width: 100%;
        }
    }

    @@media (min-width: 992px) {
        .col-aej {
            flex: 0 0 auto;
            width: 66.66666667%;
        }

        .col-va5 {
            flex: 0 0 auto;
            width: 33.33333333%;
        }
    }

    @@media all {
        .box-q8e {
            /* margin: 60px; */
        }

        .info-oak {
            background-color: #00204a;
            background-repeat: no-repeat;
            background-size: cover;
            background-position: center center;
            padding: 60px 49px 49px;
            border-radius: 10px;
        }

        .content-m4g {
            margin-bottom: 50px;
        }

        .info-dar {
            display: flex;
            align-items: center;
            gap: 17px;
            margin-bottom: 48px;
        }

        h4 {
            margin-top: 0;
            margin-bottom: .5rem;
            font-weight: 500;
            line-height: 1.2;
            color: var(--bs-heading-color);
        }

        h4 {
            font-size: calc(1.275rem + .3vw);
        }
    }

    @@media (min-width: 1200px) {
        h4 {
            font-size: 1.5rem;
        }
    }

    @@media all {
        h4 {
            color: #fff;
            font-family: "Rajdhani", sans-serif;
            font-style: normal;
            margin-bottom: 0;
            margin-top: 11px;
            line-height: 1.2;
            font-weight: 700;
            -webkit-transition: .5s;
            transition: .5s;
            font-size: 36px;
        }

        h4 {
            font-size: 24px;
        }

        .contact-6rw h4 {
            font-size: 36px;
            margin: 0 0 35px;
        }

        .content-m4g h4 {
            color: #fff;
            font-size: 30px;
            margin: 0;
        }
    }

    @@media (min-width: 992px) {
        .col-21w {
            flex: 0 0 auto;
            width: 50%;
        }

        .col-nlm {
            flex: 0 0 auto;
            width: 100%;
        }
    }

    @@media all {
        i {
            font-style: normal;
            font-weight: 400;
            list-style: none;
        }

        .icon-8kz i {
            display: inline-block;
            color: #fff;
            border: 2px solid #fff;
            width: 53px;
            height: 53px;
            border-radius: 50%;
            text-align: center;
            line-height: 53px;
        }

        .hotak:before, [class^="bi-"]:before, [class*=" bi-"]::before {
            display: inline-block;
            font-family: bootstrap-icons !important;
            font-style: normal;
            font-weight: normal !important;
            font-variant: normal;
            text-transform: none;
            line-height: 1;
            vertical-align: -.125em;
            -webkit-font-smoothing: antialiased;
            -moz-osx-font-smoothing: grayscale;
        }

        .bi-telephone-sko:before {
            content: "\f5b4";
        }

        p {
            margin-top: 0;
            margin-bottom: 1rem;
        }

        p {
            font-size: 16px;
            line-height: 26px;
            color: #fff;
            font-weight: 400;
            font-family: "Nunito", sans-serif;
            transition: 0.5s;
        }

        .content-oz3 p {
            font-size: 15px;
            color: #ffffff;
            font-weight: 400;
            margin: 0;
        }

        .content-oz3 h4 {
            font-size: 20px;
            color: #ffffff;
            font-weight: 600;
            margin: 1px 0 0;
        }

        .hotak::before, [class^="bi-"]::before, [class*=" bi-"]::before {
            display: inline-block;
            font-family: bootstrap-icons !important;
            font-style: normal;
            font-weight: normal !important;
            font-variant: normal;
            text-transform: none;
            line-height: 1;
            vertical-align: -.125em;
            -webkit-font-smoothing: antialiased;
            -moz-osx-font-smoothing: grayscale;
        }

        .bi-envelope-open-9mx:before {
            content: "\f32d";
        }

        .bi-eoh:before {
            content: "\f102";
        }

        input {
            margin: 0;
            font-family: inherit;
            font-size: inherit;
            line-height: inherit;
        }

        .input-kwi input {
            border: 2px solid rgba(4,20,36,0.1);
            width: 100%;
            height: 60px;
            padding: 0 20px;
            border-radius: 3px;
            outline: 0;
            margin-bottom: 27px;
        }

        select {
            margin: 0;
            font-family: inherit;
            font-size: inherit;
            line-height: inherit;
        }

        select {
            text-transform: none;
        }

        select {
            word-wrap: normal;
        }

        .input-kwi select {
            border: 2px solid rgba(4,20,36,0.1);
            width: 100%;
            height: 60px;
            padding: 0 20px;
            border-radius: 3px;
            outline: 0;
        }

        textarea {
            margin: 0;
            font-family: inherit;
            font-size: inherit;
            line-height: inherit;
        }

        textarea {
            resize: vertical;
        }

        .input-kwi textarea {
            border: 2px solid rgba(4,20,36,0.1);
            width: 100%;
            height: 160px;
            padding: 17px 20px;
            border-radius: 3px;
            outline: 0;
            margin-bottom: 27px;
        }

        button {
            border-radius: 0;
        }

        button {
            margin: 0;
            font-family: inherit;
            font-size: inherit;
            line-height: inherit;
        }

        button {
            text-transform: none;
        }

        [type="submit"], button {
            -webkit-appearance: button;
        }

            [type="submit"]:not(:disabled), button:not(:disabled) {
                cursor: pointer;
            }

        .button-169 button {
            border-radius: 3px;
            background-color: #00204a;
            border: 2px solid #00204a;
            color: #fff;
            padding: 15px 38px;
            transition: 0.5s;
        }

        .style-three .button-169 button:hover {
            color: #00204a;
            background: #fff;
        }
    }


    /* These were inline style tags. Uses id+class to override almost everything */
    #Ema-x9w.style-ZJdig {
        background-image: url("data:image/png; base64,") !important;
        background-repeat: no-repeat;
        background-size: 20px;
        background-position: 97% center;
        cursor: auto;
    }
</style>
<section style="background-color:#00204a !important">
    <div class="row-2fv add-white-9pe item-2d4 contactContainer">
        <div class="col-aej col-42m contactItem">
            <div class="box-q8e ">
                <div class="heading_container heading_center">
                    <h2 style="color:black" id="contactUs">
                        Contact <span>Us</span>
                    </h2>
                    <p id="contactDescription" style="color:black;">
                        lorem
                    </p>
                    <br />
                </div>
                <form method="post">
                    <div class="row-2fv">
                        <div class="col-21w">
                            <div class="input-kwi">
                                <input asp-for="Fullname" required placeholder="Your Fullname" id="fullname">
                                <span asp-validation-for="Fullname" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-21w">
                            <div class="input-kwi">
                                <input asp-for="Email" required placeholder="Enter E-Mail" class="style-ZJdig" id="email">
                                <span asp-validation-for="Email" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-21w">
                            <div class="input-kwi">
                                <input asp-for="PhoneNumber" required placeholder="Phone Number" id="phone">
                                <span asp-validation-for="PhoneNumber" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-21w">
                            <div class="input-kwi">
                                <input asp-for="Subject" required placeholder="Enter Subject" id="subject">
                                <span asp-validation-for="Subject" class="text-danger"></span>
                            </div>
                        </div>

                        <div class="col-nlm">
                            <div class="input-kwi">
                                <textarea asp-for="Message" required placeholder="Write Massage" id="message"></textarea>
                                <span asp-validation-for="Message" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-nlm">
                            <div class="button-169">
                                <button id="formButtonText" type="submit">Send Massage </button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
        <div class="col-va5 col-42m contactItem">
            <div class="info-oak ">
                <div class="content-m4g" >
                    <h4 id="infoText">Don’t Forget to Contact Us</h4>
                </div>
                <div class="info-dar">
                    <div class="icon-8kz">
                        <i class="fa-solid fa-phone-flip"></i>
                    </div>
                    <div class="content-oz3">
                        <p id="infoTextCall">Call Us</p>
                        <h4>@Model.Settings!["Telefon1"]</h4>
                    </div>
                </div>
                <div class="info-dar">
                    <div class="icon-8kz">
                        <i class="fa-solid fa-envelope"></i>
                    </div>
                    <div class="content-oz3">
                        <p id="infoTextEmail">Send E-Mail</p>
                        <h4>@Model.Settings!["Email"]</h4>
                    </div>
                </div>
                <div class="info-dar">
                    <div class="icon-8kz">
                        <i class="fa-solid fa-location-dot"></i>
                    </div>
                    <div class="content-oz3">
                        <p id="infoTextLocation">Location</p>
                        <h4>@Model.Settings!["Unvan"]</h4>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<section>
    <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d12150.943487915061!2d49.8591972!3d40.4147017!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x403087fef0e167f9%3A0x2d47dbedc1eea9e7!2sChinar%20Park!5e0!3m2!1sen!2saz!4v1729738873094!5m2!1sen!2saz" width="100%" height="500" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
</section>

@section Scripts {
    <script src="~/js/contactRender.js"></script>
}